{
  "name": "credifyy-website",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Credifyy Website - A React application for credential management",
  "author": "Your Name",
  "license": "MIT",
  "homepage": "https://credifyy.yourdomain.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/batterrathod/credifyy_Website_react.git"
  },
  "keywords": [
    "react",
    "vite",
    "tailwindcss",
    "firebase",
    "portfolio",
    "credentials"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "prebuild": "node -e \"import('fs').then(fs => { const path = './node_modules/.bin/vite'; if (fs.existsSync(path)) { fs.chmodSync(path, 0o755); } }).catch(() => {})\"",
    "render-build": "npm ci && npm run build",
    "vercel-build": "npm run build",
    "postinstall": "node -e \"console.log('âœ… Dependencies installed successfully!')\"",
    "clean": "rm -rf node_modules dist .cache",
    "clean-install": "npm run clean && npm install"
  },
  "dependencies": {
    "firebase": "^12.7.0",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.562.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.21",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.0",
    "vite": "^5.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "npm@9.6.7"
}